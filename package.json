{
  "name": "gulp-etl-target-spreadsheet",
  "version": "1.0.1",
  "repository": "https://github.com/gulpetl/gulp-etl-target-spreadsheet",
  "description": "Compose spreadsheets (XLS, XLSX, ODS, DBF, CSV, etc) from gulp-etl's Message Stream JSON format",
  "main": "dist/js/plugin.js",
  "typings": "dist/types/plugin.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rimraf dist/*",
    "prebuild": "npm run clean",
    "test": "jest",
    "defaultTask": "gulp --gulpfile ./debug/gulpfile.ts default"
  },
  "author": "peterl@sbcotton.com",
  "license": "MIT",
  "keywords": [
    "gulpplugin",
    "csv",
    "spreadsheets",
    "excel",
    "xls",
    "xlsx",
    "ods",
    "dbf",
    "gulp-etl",
    "ndjson",
    "html",
    "office"
  ],
  "homepage": "https://gulpetl.com",
  "dependencies": {
    "gulp-error-handle": "^1.0.1",
    "loglevel": "^1.6.3",
    "pkginfo": "^0.4.1",
    "plugin-error": "^1.0.1",
    "replace-ext": "^1.0.0",
    "through2": "^3.0.1",
    "vinyl": "^2.2.0",
    "xlsx": "^0.14.3"
  },
  "devDependencies": {
    "@types/jest": "^23.3.14",
    "@types/loglevel": "^1.6.3",
    "@types/node": "^10.14.13",
    "@types/vinyl": "^2.0.3",
    "gulp": "^4.0.2",
    "jest": "^24.8.0",
    "rimraf": "^2.6.3",
    "ts-jest": "^24.0.2",
    "ts-node": "^7.0.1",
    "typescript": "^3.5.3"
  }
}
