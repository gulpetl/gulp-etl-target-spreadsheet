    "version": "0.2.0",
    "configurations": [
        {
            "name": "Target Xlsx",
            "type": "node",
            "request": "launch",
            "program": "${workspaceRoot}/node_modules/gulp/bin/gulp.js",
            "stopOnEntry": false,
            "args": ["--gulpfile", "./debug/gulpfile.ts", "default"],
            "cwd": "${workspaceRoot}",
            "runtimeArgs": [
                "--nolazy", "-r", "ts-node/register"
            ],
            "console": "internalConsole",
            "env": {"DEBUG_LEVEL":"info"}
        },
        {
            "name": "Target Xlsx Debug",
            "type": "node",
            "request": "launch",
            "program": "${workspaceRoot}/node_modules/gulp/bin/gulp.js",
            "stopOnEntry": false,
            "args": ["--gulpfile", "./debug/gulpfile.ts", "default"],
            "cwd": "${workspaceRoot}",
            "runtimeArgs": [
                "--nolazy", "-r", "ts-node/register"
            ],
            "console": "internalConsole",
            "env": {"DEBUG_LEVEL":"debug"}
        },
        {
            "name" : "Target HTML",
            "type" : "node",
            "request" : "launch",
            "program" : "${workspaceRoot}/node_modules/gulp/bin/gulp.js",
            "stopOnEntry" : false,
            "args" : ["--gulpfile", "./debug/gulpfile.ts", "runHtml"],
            "cwd" : "${workspaceRoot}",
            "runtimeArgs" : [
                "--nolazy", "-r", "ts-node/register"
            ],
            "console": "internalConsole",
            "env": {"DEBUG_LEVEL":"info"}
        },
        {
            "name" : "Target ODS",
            "type" : "node",
            "request" : "launch",
            "program" : "${workspaceRoot}/node_modules/gulp/bin/gulp.js",
            "stopOnEntry" : false,
            "args" : ["--gulpfile", "./debug/gulpfile.ts", "runOds"],
            "cwd" : "${workspaceRoot}",
            "runtimeArgs" : [
                "--nolazy", "-r", "ts-node/register"
            ],
            "console": "internalConsole",
            "env": {"DEBUG_LEVEL":"info"}
        }
    ]
}